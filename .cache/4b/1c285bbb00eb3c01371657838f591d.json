{\n  background-image: url(\"./images/background.jpeg\");\n}\n.globalHeader {\n  margin: 20px;\n}\n@media (min-width: 500px) {\n  .globalHeader {\n    margin: 60px 0 100px;\n  }\n}\n.searchForm {\n  display: flex;\n  justify-content: space-between;\n}\n@media (min-width: 500px) {\n  .searchForm {\n    max-width: 400px;\n    margin-left: auto;\n    margin-right: auto;\n  }\n}\n.searchForm > input {\n  width: 100%;\n  margin-right: 10px;\n  height: 40px;\n  padding: 0 10px;\n  border: 1px solid #ddd;\n  border-radius: 12px;\n}\n.searchForm > button {\n  white-space: nowrap;\n  padding: 0 18px;\n  border: none;\n  border-radius: 12px;\n  background: #0d6aa0;\n  color: white;\n  font-size: 9px;\n}\n.globalMain {\n  max-width: 800px;\n  margin-left: auto;\n  margin-right: auto;\n}\n\n.siteList {\n  margin: 20px;\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n}\n.siteList > li {\n  margin-bottom: 10px;\n}\n@media (min-width: 500px) {\n  .siteList > li {\n    margin-right: 30px;\n  }\n}\n.siteList .site {\n  width: 100px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-direction: column;\n  background: white;\n  margin-bottom: 10px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  padding: 20px 0;\n  position: relative;\n  cursor: pointer;\n}\n.siteList .site > .logo {\n  width: 64px;\n  height: 44px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  font-size: 44px;\n  text-transform: uppercase; /*字母大写*/\n}\n.siteList .site > .link {\n  font-size: 9px;\n  margin-top: 4px;\n}\n.siteList .site > .close {\n  position: absolute;\n  right: 10px;\n  top: 5px;\n  display: none;\n  cursor: default;\n}\n.siteList .site:hover > .close {\n  display: block;\n}\n.siteList .addButton {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-direction: column;\n  width: 100px;\n  height: 117px;\n  padding: 20px 0;\n  background: white;\n  border-radius: 4px;\n}\n.siteList .addButton .icon {\n  width: 56px;\n  height: 56px;\n}\n.siteList .addButton .text {\n  font-size: 9px;\n  margin-top: 4px;\n}\n.siteList .addButton .icon-wrapper {\n  width: 64px;\n  height: 44px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n"},"lineCount":null}},"error":null,"hash":"56d159350d9cd0d9452892d41f5d7943","cacheData":{"env":{}}}